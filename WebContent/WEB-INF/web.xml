<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>EmployeeManagement</display-name>
	<servlet>
		<servlet-name>loginservelet</servlet-name>
		<display-name>loginservelet</display-name>
		<description></description>
		<servlet-class>com.cts.employee.controller.loginservelet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.cts.employee.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EmployeeRegistration</servlet-name>
		<display-name>EmployeeRegistration</display-name>
		<description></description>
		<servlet-class>EmployeeRegistration</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>employeeRegistration</servlet-name>
		<display-name>employeeRegistration</display-name>
		<description></description>
		<servlet-class>com.cts.employee.controller.employeeRegistration</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginservelet</servlet-name>
		<url-pattern>/loginservelet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeRegistration</servlet-name>
		<url-pattern>/EmployeeRegistration</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>employeeRegistration</servlet-name>
		<url-pattern>/employeeRegistration</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	   
		<welcome-file>login.html</welcome-file>
		
	</welcome-file-list>
</web-app>
